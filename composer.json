{
    "name": "rnr1721/le7-entify",
    "description": "Entify is entity framework for le7 PHP MVC framework or any PHP project",
    "keywords": ["le7", "le7-framework", "framework", "entity", "component"],
    "authors": [
        {
            "name": "Eugeny G",
            "email": "rnr1721@gmail.com",
            "homepage": "https://xhamster.biz.ua",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Core\\Entify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entities\\": "tests/Entities"
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^2.0",
        "rnr1721/le7-validator": "^1.0"
    },
    "require-dev": {
        "vimeo/psalm": "^5.7",
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@psalm"
        ],
        "phpunit": "phpunit",
        "psalm": "psalm"
    }
}
